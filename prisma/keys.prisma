// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_KEYS_URL")
}

model Key {
  id     String @id @default(uuid())
  secret String @unique

  @@map("keys")
}
